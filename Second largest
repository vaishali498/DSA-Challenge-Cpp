class Solution {
  public:
    // Function returns the second
    // largest elements
    int getSecondLargest(vector<int> &arr) {
        int largest=arr[0];
        int seclargest=-1;
        
       for(int i=0;i<arr.size();i++){
           if(arr[i]>largest){
               seclargest=largest;
               largest=arr[i];
           }
           else if(arr[i]<largest && arr[i]>seclargest){
               seclargest=arr[i];
           }
       }
        return seclargest;
       
    }
};

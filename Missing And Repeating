class solution{
public:
vector<int> findTwoElement(vector<int>& arr) {
int n = arr.size();
        for(int i=0;i<n;i++){
            arr[i]--;
        }
        for(int i=0;i<n;i++){
            arr[arr[i]%n]+=n;
        }
        
        vector<int> ans(2);
        for(int i=0;i<n;i++)
        {
            //missing
            if(arr[i]/n==2)
            ans[0]=i+1;
            //repeating
            else if(arr[i]/n==0)
            ans[1]=i+1;
        }
        return ans;
